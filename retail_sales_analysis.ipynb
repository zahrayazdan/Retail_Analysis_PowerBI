    "\n",
    "# Retail Sales Data Analysis\n"
    "\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "\n",
    "# Load the dataset\n",
    "file_path = 'SuperStoreUS_2015.xlsx'\n",
    "orders_df = pd.read_excel(file_path, sheet_name='Orders')\n",
    "\n",
    "# Convert date columns to datetime format\n",
    "orders_df['Order Date'] = pd.to_datetime(orders_df['Order Date'])\n",
    "orders_df['Ship Date'] = pd.to_datetime(orders_df['Ship Date'])\n",
    "\n",
    "# Total Sales and Profit Summary\n",
    "total_sales = orders_df['Sales'].sum()\n",
    "total_profit = orders_df['Profit'].sum()\n",
    "print(f\"Total Sales: {total_sales:.2f}\")\n",
    "print(f\"Total Profit: {total_profit:.2f}\")\n",
    "\n",
    "# Total Sales by Category\n",
    "sales_by_category = orders_df.groupby('Product Category')['Sales'].sum()\n",
    "sales_by_category.plot(kind='pie', autopct='%1.1f%%', title='Total Sales by Category')\n",
    "plt.ylabel('')\n",
    "plt.show()\n",
    "\n",
    "# Total Profit by Category and Customer Segment\n",
    "profit_by_category_segment = orders_df.groupby(['Product Category', 'Customer Segment'])['Profit'].sum().unstack()\n",
    "profit_by_category_segment.plot(kind='bar', title='Total Profit by Category & Customer Segment')\n",
    "plt.ylabel('Total Profit')\n",
    "plt.show()\n",
    "\n",
    "# Monthly Sales Trends\n",
    "orders_df['Month'] = orders_df['Order Date'].dt.month\n",
    "orders_df['Year'] = orders_df['Order Date'].dt.year\n",
    "monthly_sales = orders_df.groupby(['Year', 'Month'])['Sales'].sum().unstack()\n",
    "monthly_sales.plot(title='Total Sales by Month and Year')\n",
    "plt.ylabel('Sum of Sales')\n",
    "plt.show()\n",
    "\n",
    "# Shipping Cost Analysis by Ship Mode\n",
    "shipping_cost_by_mode = orders_df.groupby('Ship Mode')['Shipping Cost'].sum()\n",
    "shipping_cost_by_mode.plot(kind='pie', autopct='%1.1f%%', title='Shipping Cost by Ship Mode')\n",
    "plt.ylabel('')\n",
    "plt.show()\n",
    "\n",
    "# Sales by Market and Ship Mode\n",
    "sales_by_market_ship_mode = orders_df.groupby(['Region', 'Ship Mode'])['Sales'].sum().unstack()\n",
    "sales_by_market_ship_mode.plot(kind='barh', title='Sales by Region & Ship Mode')\n",
    "plt.xlabel('Total Sales')\n",
    "plt.show()\n",
    "\n",
    "# Quantity Analysis by Month and Category\n",
    "quantity_by_month_category = orders_df.groupby(['Month', 'Product Category'])['Quantity ordered new'].sum().unstack()\n",
    "quantity_by_month_category.plot(title='Quantity by Month & Product Category')\n",
    "plt.ylabel('Sum of Quantity')\n",
    "plt.show()\n",
    "\n",
    "# Profit Analysis by Year\n",
    "profit_by_year = orders_df.groupby('Year')['Profit'].sum()\n",
    "profit_by_year.plot(kind='bar', title='Total Profit by Year')\n",
    "plt.ylabel('Sum of Profit')\n",
    "plt.show()\n"
   ]
